# Ragdoll Demo Environment Variables
# Copy this file to .env and fill in your values

# Database
DATABASE_URL=postgresql://localhost/ragdoll_demo_development

# Rails
RAILS_ENV=development
RAILS_LOG_LEVEL=debug

# Foreman
PORT=3000
TIMEOUT=20

# Ragdoll Configuration (optional - will use defaults if not set)
# RAGDOLL_EMBEDDING_MODEL=text-embedding-ada-002
# RAGDOLL_LLM_MODEL=gpt-3.5-turbo
# RAGDOLL_API_KEY=your_openai_api_key_here

# Background Jobs
SOLID_QUEUE_BATCH_SIZE=10
SOLID_QUEUE_THREADS=5

# Development tools
LIVERELOAD=true